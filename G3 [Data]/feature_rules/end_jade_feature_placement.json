{
	"format_version": "1.13.0",
	
	"minecraft:feature_rules": {
		"description": {
			"identifier": "g3:end_jade_feature_placement",
			
			"places_feature": "g3:end_jade_feature"
		},
		
		"conditions": {
			"placement_pass": "underground_pass"
		},
		
		"distribution": {
			"iterations": "math.abs(v.originx) > 500 || math.abs(v.originz) > 500",
			"scatter_chance": 0.5,
			"x": {
				"distribution": "uniform",
				"extent": [0, 16]
			},
			"z": {
				"distribution": "uniform",
				"extent": [0, 16]
			},
			"y": "query.heightmap(v.worldx, v.worldz) - math.random_integer(16, 24)"
		}
	}
}